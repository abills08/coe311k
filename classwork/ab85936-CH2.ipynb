    "def divide(number):\n",
    "    quotient = str(number % 2)\n",
    "    number = number // 2\n",
    "    while number >= 1:\n",
    "        quotient = str(number % 2) + \" \" + quotient\n",
    "        number = number // 2\n",
    "    return quotient\n",
    "       \n",
    "print(divide(2026))"
